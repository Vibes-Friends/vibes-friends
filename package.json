{
  "name": "vibes-friends",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "dev:landing": "pnpm --filter @vibes/landing dev",
    "dev:forms": "pnpm --filter @vibes/forms dev",
    "build": "turbo build",
    "build:landing": "pnpm --filter @vibes/landing build",
    "build:forms": "pnpm --filter @vibes/forms build",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "turbo": "^2.4.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
